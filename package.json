{
  "name": "Mixceed",
  "version": "1.0.0",
  "description": "Mixceed",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/kanehara/mixceed.git"
  },
  "keywords": [
    "spotify",
    "music",
    "mixceed"
  ],
  "author": "Yohei Kanehara <kanehara@umich.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/kanehara/mixceed/issues"
  },
  "homepage": "https://gitlab.com/kanehara/mixceed#README",
  "scripts": {
    "dev": "wrangler dev",
    "client:ci": "cd client && npm ci --legacy-peer-deps --ignore-scripts && npm rebuild node-sass",
