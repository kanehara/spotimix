    "deploy": "wrangler deploy",
    "build:client": "cd client && npm run build"
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "wrangler": "^4.24.0"
  }
